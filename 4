using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication15
{
    class Program
    {
        static void Main(string[] args)
        {
            int a, b, c, x, y, s, S;
            Console.Write("Введите A: ");
            a = int.Parse(Console.ReadLine());
            Console.Write("Введите B: ");
            b = int.Parse(Console.ReadLine());
            Console.Write("Введите C: ");
            c = int.Parse(Console.ReadLine());
            x = a / c;
            y = b / c;
            x = x * y;
            S = a * b;
            s = c * c * x;
            s = S - s;
            Console.Write("В данном прямоугольнике поместится ");
            Console.Write(x);
            if (x==1)
            {
                Console.WriteLine(" квадрат");
            }
            else
            {
                if (x>1 && x<=4)
                {
                    Console.WriteLine(" квадрата");
                }
                else
                {
                    Console.WriteLine(" квадратов");
                }
            }
            Console.Write("Свободная от");
            if (x == 1)
            {
                Console.Write(" квадрата");
            }
            else
            {
                Console.Write(" квадратов");
            }
            Console.Write(" площадь равна: ");
            Console.WriteLine(s);
            Console.ReadKey();
        }
    }
}
