using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication15
{
    class Program
    {
        static void Main(string[] args)
        {
            double N;
            Console.Write("Введите количество секунд, прошедших с начала дня ");
            N = double.Parse(Console.ReadLine());
            Console.Write("С начала последней минуты прошло ");
            Console.Write(N % 60);
            Console.WriteLine(" с");
            Console.ReadKey();
        }
    }
}
